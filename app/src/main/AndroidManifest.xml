<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solitary.ks">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".activity.PositionDetailActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.PositionListActivity" />


        </activity>

        <activity
            android:name="com.solitary.ks.activity.HomePageActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="com.solitary.ks.activity.PositionListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />


        </activity>

        <activity
            android:name="com.solitary.ks.activity.GalleryViewActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name="com.solitary.ks.activity.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.solitary.ks.activity.KissListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />


        </activity>

        <activity
            android:name="com.solitary.ks.activity.KissDetailActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.KissListActivity" />

        </activity>
        <activity
            android:name="com.solitary.ks.activity.MassageActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />

        </activity>

        <activity
            android:name="com.solitary.ks.activity.SpotActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />
        </activity>

    <activity
        android:name="com.solitary.ks.activity.FavouritePositionListActivity"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar"
        android:screenOrientation="portrait">

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.solitary.ks.activity.HomePageActivity" />
    </activity>
        <activity
            android:name="com.solitary.ks.activity.TriedPositionsList"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />
        </activity>
        <activity
            android:name="com.solitary.ks.activity.PrivacyPolicyActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solitary.ks.activity.HomePageActivity" />
        </activity>

        <service android:name=".firebase.MyFirebaseMessagingService" android:stopWithTask="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

</application>

</manifest>